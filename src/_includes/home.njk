{% extends "base.njk" %}

{% block pagecontent %}
    
    <header class="hero">
        <h1>Hello!</h1>
        <h2>I'm Tobias</h2>
    </header>

    {% for project in collections.projects %}

        <div class="project-card">
            <a href="{{ project.url }}">{{ project.data.title }}</a>
            <img src="{{ project.data.img }}" alt="">
        </div>

    {% endfor %}
{% endblock %}